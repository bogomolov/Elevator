<h1>Listing app_seasons</h1>

<table>
  <tr>
    <th>Year</th>
    <th>Active</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_seasons.each do |app_season| %>
  <tr>
    <td><%= app_season.year %></td>
    <td><%= app_season.active %></td>
    <td><%= link_to 'Show', app_season %></td>
    <td><%= link_to 'Edit', edit_app_season_path(app_season) %></td>
    <td><%= link_to 'Destroy', app_season, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App season', new_app_season_path %>
