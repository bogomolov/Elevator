<h1>Listing app_actions</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>App module</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_actions.each do |app_action| %>
  <tr>
    <td><%= app_action.name %></td>
    <td><%= app_action.description %></td>
    <td><%= app_action.app_module_id %></td>
    <td><%= link_to 'Show', app_action %></td>
    <td><%= link_to 'Edit', edit_app_action_path(app_action) %></td>
    <td><%= link_to 'Destroy', app_action, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App action', new_app_action_path %>
