<h1>Listing app_suppliers</h1>

<table>
  <tr>
    <th>Short name</th>
    <th>Full name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @app_suppliers.each do |app_supplier| %>
  <tr>
    <td><%= app_supplier.short_name %></td>
    <td><%= app_supplier.full_name %></td>
    <td><%= link_to 'Show', app_supplier %></td>
    <td><%= link_to 'Edit', edit_app_supplier_path(app_supplier) %></td>
    <td><%= link_to 'Destroy', app_supplier, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New App supplier', new_app_supplier_path %>
